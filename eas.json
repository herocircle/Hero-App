{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "cache": {
        "key": "v1",
        "paths": ["node_modules/**", "android/app/build/**", "ios/Pods/**"]
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "preview2"
    },
    "preview3": {
      "developmentClient": true,
      "channel": "preview3"
    },
    "production": {
      "android": {
        "image": "latest"
      },
      "cache": {
        "key": "v1",
        "paths": ["android/app/build/**", "ios/Pods/**"]
      },
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "cli": {
    "appVersionSource": "remote"
  }
}
